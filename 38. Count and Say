class Solution {
public:
    string countAndSay(int n) {
        if(n<=1) return "1";
        string s = "1";
        int same, q = 1;
        same = 0;
        while(q++ < n){
        string copy = "";
        for(int k = 0; k< s.length();k++){
        int count = 1;
        while(s[k]==s[k+1] && k < s.length()-1) {k++; count++;}
        copy += to_string(count)+s[k];
        }
        s = copy;
        }
        return s ;
    }
};
