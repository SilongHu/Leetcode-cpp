class Solution {
public:
    int climbStairs(int n) {
        if(n ==0) return 1;
        if(n <=3) return n;
        int first = 2, second = 3, next;
        for(int c = 3 ; c < n ; c++ ){
            next = first + second;
            first = second;
            second = next;
        }
    return next;
    }
};
